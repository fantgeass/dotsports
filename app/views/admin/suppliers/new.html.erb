<div data-hook="admin_supplier_new_form_header">
  <%= render 'shared/error_messages', :target => @supplier %>
  <h1><%= t(:new_supplier) %></h1>
</div>

<div data-hook="admin_supplier_new_form">
  <%= form_for [:admin, @supplier] do |f| %>
    <%= render 'form', :f => f %>

    <div data-hook="admin_supplier_new_form_buttons">
      <%= render 'admin/shared/new_resource_links' %>
    </div>
  <% end %>
</div>
